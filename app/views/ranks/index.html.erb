<p id="notice"><%= notice %></p>

<h1>Listing Ranks</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>All pv</th>
      <th>Average readable</th>
      <th>Average benefit</th>
      <th>Rank pv</th>
      <th>Rank readable</th>
      <th>Rank benefit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ranks.each do |rank| %>
      <tr>
        <td><%= rank.user_id %></td>
        <td><%= rank.all_pv %></td>
        <td><%= rank.average_readable %></td>
        <td><%= rank.average_benefit %></td>
        <td><%= rank.rank_pv %></td>
        <td><%= rank.rank_readable %></td>
        <td><%= rank.rank_benefit %></td>
        <td><%= link_to 'Show', rank %></td>
        <td><%= link_to 'Edit', edit_rank_path(rank) %></td>
        <td><%= link_to 'Destroy', rank, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rank', new_rank_path %>
